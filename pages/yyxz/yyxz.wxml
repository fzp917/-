<scroll-view scroll-y class="scrollPage">
  <view class='nav-list'>
    <view hover-class='none' class="nav-li" wx:for="{{elements}}" wx:key>
    <view class="nav-title">
      <text>{{item.title}}</text>
    </view>
    <view class="nav-name">
      <text bindtap="bindViewItem" data-item="{{item}}">查看</text>
    </view>
    </view>
  </view>
</scroll-view>